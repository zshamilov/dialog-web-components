{
  "name": "@dlghq/dialog-components",
  "version": "0.78.2",
  "description": "Dialog Web Components",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "react",
    "components",
    "dialog"
  ],
  "scripts": {
    "start": "styleguidist server",
    "create": "node scripts/create-component.js",
    "build": "npm-run-all --parallel build:*",
    "build:l10n": "node scripts/compile-translations.js",
    "pretest": "npm run lint",
    "test": "exit 0",
    "lint": "npm-run-all lint:js lint:flow",
    "lint:js": "eslint src/",
    "lint:flow": "flow check",
    "lint:css": "stylelint 'src/**/*.css'",
    "format": "npm-run-all --parallel format:*",
    "format:js": "eslint src/ --fix",
    "format:css": "find src -path '**/*.css' -exec stylefmt {} \\;",
    "preversion": "npm test",
    "postversion": "npm run release",
    "postpublish": "git push --follow-tags",
    "prerelease": "npm run build",
    "release": "npm publish",
    "postrelease": "npm run docs",
    "predocs": "styleguidist build",
    "docs": "buildbranch gh-pages styleguide"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dialogs/dialog-web-components.git"
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dialogs/dialog-web-components/issues"
  },
  "homepage": "https://dialogs.github.io/dialog-web-components",
  "dependencies": {
    "@dlghq/dialog-types": "1.6.1",
    "@dlghq/dialog-utils": "0.4.1",
    "@dlghq/markdown": "0.5.1",
    "@dlghq/react-l10n": "0.6.0",
    "classnames": "2.2.5",
    "clipboard": "1.5.15",
    "immutable": "3.8.1",
    "lodash": "4.17.2",
    "photoswipe": "4.1.1",
    "react": "15.4.1",
    "react-addons-css-transition-group": "15.4.1",
    "react-addons-shallow-compare": "15.4.1",
    "react-dom": "15.4.1",
    "react-draggable": "2.2.3",
    "react-hotkeys": "0.9.0",
    "react-modal": "1.5.2",
    "react-tether": "0.5.2",
    "react-virtualized": "8.7.1"
  },
  "devDependencies": {
    "@dlghq/babel-preset-dialog": "2.5.1",
    "@dlghq/eslint-config-dialog": "1.17.0",
    "@dlghq/postcss-dialog": "1.5.0",
    "@dlghq/stylelint-config-dialog": "0.10.0",
    "buildbranch": "1.0.0",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "ejs": "2.5.3",
    "eslint": "3.11.1",
    "eslint-plugin-react": "6.8.0",
    "file-loader": "0.9.0",
    "flow-bin": "0.36.0",
    "glob": "7.1.1",
    "inquirer": "1.2.3",
    "jest-cli": "17.0.3",
    "npm-run-all": "3.1.2",
    "postcss-loader": "1.2.0",
    "pre-commit": "1.1.3",
    "raw-loader": "0.5.1",
    "react-styleguidist": "4.4.1",
    "stylefmt": "5.0.2",
    "stylelint": "7.6.0",
    "webpack": "1.13.3",
    "yml-loader": "1.1.0"
  },
  "pre-commit": {
    "colors": true,
    "run": "test"
  }
